          </div>

          <div class="cursor-pointer md:hidden nav__open">
            <svg xmlns="http://www.w3.org/2000/svg" class="w-8 h-8" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
              <path stroke-linecap="round" stroke-linejoin="round" d="M4 6h16M4 12h16m-7 6h7" />
            </svg>
          </div>
        </div>
      </div>
      <aside class="absolute w-screen text-xs bg-neutral-200 nav__topbar">
        <div class="container flex items-center justify-center py-2">
          <div class="flex items-center space-x-8 font-light md:space-x-16 lg:space-x-24">
            <span>FREE DELIVERY</span>
            <span>NEXT DAY SHIPPING</span>
            <span>FREE RETURNS</span>
          </div>
        </div>
      </aside>
    </div>
    <div class="w-full h-24"></div>
  </nav>
  <nav class="fixed top-0 left-0 z-50 w-screen min-h-screen nav__drawer bg-primary">
    <div class="container flex items-center justify-end py-8 cursor-pointer nav__close">
      <svg xmlns="http://www.w3.org/2000/svg" class="w-8 h-8 " fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
        <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
      </svg>
    </div>
    <div class="container p-20">
      <ul class="text-4xl xs:text-5xl sm:text-6xl whitespace-nowrap">
        {%- for link in linklists.main-menu.links -%}
          <li class="hover:-translate-y-1.5 duration-300">
            <a href="{{ link.url }}" {% if link.active %}aria-current="page"{% endif %} class="block w-full leading-normal">
              {% if link.title == 'Sale' %}
                <span class="text-red-400">
                  {{ link.title }}
                </span>
              {% else %}
                <span>
                  {{ link.title }}
                </span>
              {% endif %}
            </a>
    
            {%- if link.links != blank -%}
              <ul class="">
                {%- for child_link in link.links -%}
                  <li>
                    <a href="{{ child_link.url }}" {% if child_link.active %}aria-current="page"{% endif %}>
                      {{ child_link.title }}
                    </a>
    
                    {%- if child_link.links != blank -%}
                      <ul>
                        {%- for grandchild_link in child_link.links -%}
                          <li>
                            <a href="{{ grandchild_link.url }}" {% if grandchild_link.active %}aria-current="page"{% endif %}>
                              {{ grandchild_link.title }}
                            </a>
                          </li>
                        {%- endfor -%}
                      </ul>
                    {%- endif -%}
    
                  </li>
                {%- endfor -%}
              </ul>
            {%- endif -%}
    
          </li>
        {%- endfor -%}
      </ul>

      <div class="mt-16">
        {% render 'component-socials' %}
      </div>

    </div>
  </nav>
</div>

{% schema %}
{
  "name": "Header",
  "tag": "header",
  "class": "header",
  "settings": []
}
{% endschema %}
