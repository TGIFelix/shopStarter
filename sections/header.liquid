<div data-section-id="{{- section.id -}}" class="">
  <nav class="relative">
    <div class="fixed z-50 w-screen font-serif bg-light fixed__nav">
      <div class="container flex items-center justify-between py-4 fixed__nav-wrapper">
        <div class="flex items-center space-x-12">
          <a href="/" class="w-16 h-16 rounded-full bg-primary nav__brand"></a>
          <ul class="items-center justify-center hidden space-x-12 text-lg xl:text-xl md:flex">
            {%- for link in linklists.main-menu.links -%}
              <li>
                <a href="{{ link.url }}" {% if link.active %}aria-current="page"{% endif %}>
                  {% if link.title == 'Sale' %}
                    <span class="text-red-400">
                      {{ link.title }}
                    </span>
                  {% else %}
                    <span>
                      {{ link.title }}
                    </span>
                  {% endif %}
                </a>
        
                {%- if link.links != blank -%}
                  <ul>
                    {%- for child_link in link.links -%}
                      <li>
                        <a href="{{ child_link.url }}" {% if child_link.active %}aria-current="page"{% endif %}>
                          {{ child_link.title }}
                        </a>
        
                        {%- if child_link.links != blank -%}
                          <ul>
                            {%- for grandchild_link in child_link.links -%}
                              <li>
                                <a href="{{ grandchild_link.url }}" {% if grandchild_link.active %}aria-current="page"{% endif %}>
                                  {{ grandchild_link.title }}
                                </a>
                              </li>
                            {%- endfor -%}
                          </ul>
                        {%- endif -%}
        
                      </li>
                    {%- endfor -%}
                  </ul>
                {%- endif -%}
        
              </li>
            {%- endfor -%}
          </ul>
        </div>
  
        <div class="flex items-center space-x-4">
          <div class="hidden md:block">
